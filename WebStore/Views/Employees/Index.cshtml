@model IEnumerable<WebStore.Models.Employee>

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.LastName</td>
                    <td>@employee.FirstName</td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                    <td><a href="~/Home/Employee?id=@employee.Id">Инфо.</a></td>
                </tr>
            }
        </tbody>
    </table>
    <a asp-controller="Home" asp-action="Index">На главную</a>
</div>